<%= simple_form_for([@game, @rental]) do |f| %>
  <%= f.input :start_date,
  as: :date,
  start_year: Date.today.year,
  end_year: Date.today.year + 2 %>
  <%= f.input :end_date,
  as: :date,
  start_year: Date.today.year,
  end_year: Date.today.year + 5 %>
  <%= f.submit "rent this game", :class => "btn btn-primary" %>
<% end %>
